
/**************************
   컨텐츠 스타일 적용 부분
*****************************/

a{
   text-decoration: none;
}  
div#content {         
   text-align: center;
   margin-bottom: 0px;
   margin: auto;
}

div#container>img{   
   height: 170px;
   width: 100%;  
   margin-bottom: 60px; 

}
.plan_tab {
    width: 100%;
    overflow: hidden;
    clear: both;
    margin-bottom: 28px;
}


.plan_tab a {		/* 강아지 | 고양이  */
    float: left;
    width: 50%;
    height: 56px;
    line-height: 54px;
    background: #f7f9fd;
    text-align: center;
    font-size: 18px;
    font-weight: 300;
    color: #3c465e;
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    border: 1px solid #d8dce5;
    border-width: 1px 1px 1px 0;
    }
a.cat_btn{
background: #373536;
    border-color: #373536;
    color: #fff;
    font-weight: 400;
}
.main_goods_cont{
	width: 1200px;
    position: relative;
    margin: 0 auto;
    text-decoration: none;
}
.main_goods_tab ul li a {
	/* border: 1px solid blue; */
    display: block;
    height: 47px;
    /* width: 380px; */
    line-height: 45px;
    background-color: #fff;
    text-align: center;
    font-size: 16px;
    font-weight: 300;
    color: #838383;
    letter-spacing: -1px;
    text-decoration: none;
   /*  margin-left:-10px; */
    
}

.main_goods_tab {
    width: 100%;
    margin-bottom: 38px;
    border-top: 2px solid #170709;
    border-bottom: 1px solid #a1a3ae;
    
}
.main_goods_tab ul li a span {
    display: inline-block;
    vertical-align: top;
    height: 47px;
    line-height: 45px;
    font-size: 16px;
    font-weight: 300;
    color: #838383;
}
dl, ul, ol, li {
list-style: none;
}
.main_goods_tab ul li.on a {	/* 카테고리 선택시 */
    background-color: #ffc71d;
    width: 610px;
}


.main_goods_tab ul li.icon2,
.main_goods_tab ul li.icon3{
	width: 610px;
    margin-left: -10px;
}
.main_goods_tab ul li{
	float:left;
}
.main_goods_tab ul {
    width: 100%;
    overflow: hidden;
    clear: both;
    margin:0px 80px 0px -30px;
}

/********* vet5 css *********/

div.feed>ul{
	/* border: 2px solid blue; */
	margin: 0 auto;
}

div.feed_all ul li {
    display: inline-block;
    vertical-align: top;
    background: #e2e2e2;
    width: 1100px;
    height: 550px;
    margin: 0 auto;
 	margin-left:8px;
    margin-bottom:50px;
}

div.feed_all>ul>h1{
 	font-family:"나눔고딕";
	text-align: center;
    font-size:22px;
	padding:10px;
	width:300px;
	height:20px;
	margin-left: 400px;	
	color:#464646; 
}

div.feed_all>ul>P{		/* 당신의 반려견에게는...  */
	text-align:center;
	font-size:20px;
	margin-bottom: 30px;
}

div.feed_tit>h3{	/* 사료 추천결과 글씨  */
	text-align:center;
	color: #464646;
	font-size:30px;
	margin-top:40px;
	margin-bottom:45px;
	
}

button#btnReturn.feed-return{	/* 다시 계산히기 버튼 */
	border:3px solid #ffc71d;
	background:white;
	margin-left: 480px;
	width:150px;
	height:50px;
	border-radius: 4px;
	margin-top:-10px;
	margin-bottom:80px;
	cursor: pointer;
	
}
div.feed{				/* 사료결과 내용 전체 */
	/* border: 2px solid blue; */
	width: 290px;
	height: 380px;
	/* padding:20px; */
	margin-left:10px;
	display: inline-block;
}

div.feed>img#img{	/* 사료 이미지 */
	/* border: 3px solid green; */
	width:220px;
	height:230px;
	margin-left:50px;
	border-radius: 15px;
}

label.feed_tit_ {		/* 사료내용 라벨 */
	/* border:2px solid blue; */
	display: block;
	margin-bottom: 8px;
	margin-top:10px;
	color: black;
	font-size: 16px;
	margin-left: 50px; 
	width:200px;
	padding:10px; 
	text-align:center; 
}

button#btnShoppingBasket.ShoppingBasket{	/*장바구니 버튼 */
	border: 0;
	background:#56754e;
	width: 100px;
	height:30px;
	margin-left:110px;
	cursor: pointer;
}
#btnShoppingBasket>span{	/* 장바구니 글씨 */
	color:white;
}

div.feed:nth-child(2){
 	/* border: 2px solid red; */
 	margin-left: 90px;
}

