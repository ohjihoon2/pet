/** 로그인 화면 **/
div#login {
	width: 100%;
	display: inline-block;
	text-align: center;
}

div#login form {
	border: 1px solid #ccc;
	display: inline-block;
	height: 350px;
	width: 450px;
	margin-top: 100px;
	margin-bottom: 150px;
	padding: 45px 60px 80px 60px;
	width: 450px;
}

div#login form>span:nth-child(1) {
	/* border: 1px solid red; */
	display: block;
	font-size: 26px;
	color: #353535;
}

div#login form>input#id {
	margin: 0 auto;
	border: 1px solid #ccc;
	width: 385px;
	height: 45px;
	margin-top: 50px;
	display: block;
	padding-left: 15px;
}

div#login form>input#pass {
	margin: 0 auto;
	border: 1px solid #ccc;
	width: 385px;
	height: 45px;
	margin-top: 15px;
	display: block;
	margin-bottom: 5px;
	padding-left: 15px;
}

div#login form>div:nth-child(4) {
	width: 400px;
	text-align: left;
	margin: auto;
}

div#login form>div:nth-child(4) a {
	color: #9a9a9a;
	font-size: 13px;
}

div#login form>div:nth-child(4) a span {
	color: #9a9a9a;
	font-family: "나눔고딕";
	font-size: 12px;
}

div#login form>div:nth-child(4) a:nth-child(2) {
	margin-left: 120px;
	margin-bottom: 10px;
}

div#login form>button {
	border: 1px solid #ffc71d;
	width: 400px;
	height: 60px;
	background-color: #ffc71d;
	margin-top: 40px;
	color: white;
	font-size: 20px;
	font-weight: bold;
}

div#login form>button:hover {
	cursor: pointer;
}

