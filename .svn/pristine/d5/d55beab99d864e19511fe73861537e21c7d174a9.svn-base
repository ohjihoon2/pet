div#join form>label.radio1 {
	margin-left: 73px;
	margin-top: 10px;
	overflow: hidden;
	display: inline-block;
	position: relative;
	height: 100px;
	width: 100px;
	box-sizing: border-box;
	cursor: pointer;
}

.radio1 input {
	overflow: hidden;
	display: none;
	width: 0px;
	height: 0px;
	border: 0 none;
	font-size: 0;
	line-height: 0;
	clip: rect(0, 0, 0, 0);
	opacity: 0;
}

.radio1 .ico1 {
	position: absolute;
	left: 0px;
	top: 0px;
	width: 100px;
	height: 100px;
	background: url("http://localhost:9090/pet/images/dogico1.png")
		no-repeat 0 0;
}

.radio1 input:checked+.ico1 {
	background: url("http://localhost:9090/pet/images/dogico2.png")
		no-repeat 0 0;
}

div#join form>label.radio2 {
	margin-left: 104px;
	margin-top: 10px;
	overflow: hidden;
	display: inline-block;
	position: relative;
	height: 100px;
	width: 100px;
	box-sizing: border-box;
