package com.pettoday.vo;

public class WonderVO {

	String wonder_title, wonder_content, wonder_date, wonder_writer;
	int wonder_no, wonder_view,rno;
	
	
	
	public int getRno() {
		return rno;
	}
	public void setRno(int rno) {
		this.rno = rno;
	}
	public String getWonder_title() {
		return wonder_title;
	}
	public void setWonder_title(String wonder_title) {
		this.wonder_title = wonder_title;
	}
	public String getWonder_content() {
		return wonder_content;
	}
	public void setWonder_content(String wonder_content) {
		this.wonder_content = wonder_content;
	}
	public String getWonder_date() {
		return wonder_date;
	}
	public void setWonder_date(String wonder_date) {
		this.wonder_date = wonder_date;
	}
	public String getWonder_writer() {
		return wonder_writer;
	}
	public void setWonder_writer(String wonder_writer) {
		this.wonder_writer = wonder_writer;
	}
	public int getWonder_no() {
		return wonder_no;
	}
	public void setWonder_no(int wonder_no) {
		this.wonder_no = wonder_no;
	}
	public int getWonder_view() {
		return wonder_view;
	}
	public void setWonder_view(int wonder_view) {
		this.wonder_view = wonder_view;
	}
	
	
}
