package com.pettoday.vo;

public class AnswerVO {

	 
	int wonder_answer_no, wonder_no;
	String wonder_answer_title, wonder_answer_content , wonder_answer_date ,wonder_answer_writer;
	int answer_count;
	
	public int getAnswer_count() {
		return answer_count;
	}
	public void setAnswer_count(int answer_count) {
		this.answer_count = answer_count;
	}
	public String getWonder_answer_title() {
		return wonder_answer_title;
	}
	public void setWonder_answer_title(String wonder_answer_title) {
		this.wonder_answer_title = wonder_answer_title;
	}
	public int getWonder_answer_no() {
		return wonder_answer_no;
	}
	public void setWonder_answer_no(int wonder_answer_no) {
		this.wonder_answer_no = wonder_answer_no;
	}
	public int getWonder_no() {
		return wonder_no;
	}
	public void setWonder_no(int wonder_no) {
		this.wonder_no = wonder_no;
	}
	public String getWonder_answer_content() {
		return wonder_answer_content;
	}
	public void setWonder_answer_content(String wonder_answer_content) {
		this.wonder_answer_content = wonder_answer_content;
	}
	public String getWonder_answer_date() {
		return wonder_answer_date;
	}
	public void setWonder_answer_date(String wonder_answer_date) {
		this.wonder_answer_date = wonder_answer_date;
	}
	public String getWonder_answer_writer() {
		return wonder_answer_writer;
	}
	public void setWonder_answer_writer(String wonder_answer_writer) {
		this.wonder_answer_writer = wonder_answer_writer;
	}
	@Override
	public String toString() {
		return "AnswerVO [wonder_answer_no=" + wonder_answer_no + ", wonder_no=" + wonder_no + ", wonder_answer_title="
				+ wonder_answer_title + ", wonder_answer_content=" + wonder_answer_content + ", wonder_answer_date="
				+ wonder_answer_date + ", wonder_answer_writer=" + wonder_answer_writer + ", answer_count="
				+ answer_count + "]";
	}
	
	
	
}
