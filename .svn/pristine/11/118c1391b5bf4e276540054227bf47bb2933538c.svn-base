a {
	text-decoration: none;
	color: black;
}

/** 사진대회 화면 **/
div#contest {
	width: 100%;
	display: inline-block;
	text-align: center;
}

div#contest_all {
	width: 1200px;
	margin: auto;
	margin-top: 0px;
}

/**사진대회 TOP3**/
div#top3 {
	height: 435px;
	margin-top: 50px;
	border-bottom: 1px solid #ccc;
	margin-bottom: 50px;
}

div#top3>label {
	border-bottom: 1px solid #ccc;
	font-size: 26px;
	color: #353535;
	display: block;
	height: 40px;
	margin-bottom: 40px;
}

div#top3>div {
	display: inline-block;
	margin-right: 20px;
	width: 375px;
	text-align: left;
	font-size: 15px;
}

div#top3>div:nth-child(4) {
	margin-right: 0px;
}

div#top3>div>img:nth-child(1) {
	position: absolute;
	margin: 10px;
	z-index: 9999;
}

div#top3>div>div {
	overflow: hidden;
	border-radius: 7px;
	margin-bottom: 10px;
}

div#top3>div>div>a img {
	width: 375px;
	height: 300px;
}

div#top3>div>div>a img:hover {
	cursor: pointer;
	border-radius: 7px;
	transform: scale(1.1); /* 마우스 오버시 이미지 크기를 1.1 배만큼 확대시킨다. */
	-o-transform: scale(1.1);
	-moz-transform: scale(1.1);
	-webkit-transform: scale(1.1);
	transition: transform .35s;
	-o-transition: transform .35s;
	-moz-transition: transform .35s;
	-webkit-transition: transform .35s;
	z-index: 10;
}

div#top3>div>span:nth-child(4) {
	margin-left: 197px;
}

