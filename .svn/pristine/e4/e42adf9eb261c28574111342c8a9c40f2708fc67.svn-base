a{
	 cursor: pointer;
	 text-decoration: none;
}
div#content{
	width:1611.8px;
	height:1230px;
	float: right;
	border-left: 1px solid #cccccc;
	background-color: #eeeeee;
	margin: 0px;
}
div#item_content{
	text-align: center;
}
div#dog_item{
	width: 600px;
	height: 400px;
	border: 1px solid #cccccc;
	border-radius:10px;
	display: inline-block;
	background-color: white;
	margin-top: 50px;
	text-align: left;
}
div#dog_item h3{
	margin: 20px 0 0 20px;
		display: inline-block;
}
div#dog_item h4{
	margin: 24px 10px 0 0;
	float: right;
	display: inline-block;
	font-size: 11pt;
	color: #8e8e8e;
}

