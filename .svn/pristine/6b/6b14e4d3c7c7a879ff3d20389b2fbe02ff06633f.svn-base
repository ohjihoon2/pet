    font-size: 27px;
    margin-top:70px;
    margin-bottom: 45px;
}

div.meet,
div.allergy,
div.mouthfeel,
div.product{
	/* border:2px solid red; */
	margin-left: 480px;
	margin-top:-40px;
}

select {
    padding: 0 0 0 25px;
    color: #666;
    font-size: 15px;
    margin-left: 480px;
