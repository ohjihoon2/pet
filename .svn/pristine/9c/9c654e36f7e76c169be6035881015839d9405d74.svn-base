div#con_item {
	/* border:1px solid red; */
	margin: 0 auto;
	width: 1200px;
	overflow: hidden;
}

/** 
	aside start
 **/
div#con_item aside{
	width: 200px;
	height: 500px;
	/* border:1px solid black; */ 
	float:left;
	margin-top: 50px;
}

div#con_item aside div#asidemenu:nth-child(1){
	width: 160px;
	border: 1px solid #ededed;
	padding: 0 20px 0 20px;
}


div#con_item aside div#asidemenu:nth-child(1) ul:nth-child(2){
	list-style: none;
	padding: 0;
	margin: 0 0 20px 0;
}

div#con_item aside div#asidemenu:nth-child(1) ul:nth-child(5){
	list-style: none;
	padding: 0;
	margin: 0;
	/* border:1px solid blue; */
}

div#con_item aside div#asidemenu:nth-child(1) ul li:nth-child(1){
	margin-top:25px;
}

div#con_item aside div#asidemenu:nth-child(1) ul li{
	list-style: none;
	display: block;
	/* border: 1px solid red; */
	padding: 0;
	margin: 15px 0px;
}

div#con_item aside div#asidemenu:nth-child(1) ul li a{
	display:inline-block;
	width:100%;
	font-size:14px;
	color:#333333;
	text-decoration: none;
	/* border: 1px solid black; */ 
}

div#con_item aside div#asidemenu:nth-child(1) ul li:hover{
	/* background-color: green; */
	color: #e19404;
	cursor: pointer;
}

div#con_item aside div#asidemenu:nth-child(1) h3{
	font-size:20px;
	color:#333333;
	padding: 0;
	margin: 20px 0 10px 0;
	cursor: pointer;
}

div#con_item aside div>hr{
	border: 1px solid #ededed;
}

div#con_item aside div#asidemenu #asidedog1 img{
	float: right;
}

div#con_item aside div#asidemenu #asidecat1 img{
	float: right;
}
/** 
	aside end
 **/
 
 
 
