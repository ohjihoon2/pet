div#con_item section:nth-child(3) article#sp33 div.box_sequence2 ul{
	/*border:1px solid red;*/
	width:100%;
	padding:0px;
	margin:0x;
}

div#con_item section:nth-child(3) article#sp33 div.box_sequence2 ul li.sequence2_li{
	border:1px solid #e2e2e2;
