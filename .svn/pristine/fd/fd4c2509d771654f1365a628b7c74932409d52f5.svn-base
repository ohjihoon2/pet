/**************************
   컨텐츠 스타일 적용 부분
*****************************/

a{
   text-decoration: none;
}  
div#content {         
   text-align: center;
   margin-bottom: 0px;
   margin: auto;
}

div#container>img{   
   height: 360px;
   width: 100%;  
   margin-bottom: 60px; 

}
.plan_tab {
    width: 100%;
    overflow: hidden;
    clear: both;
    margin-bottom: 28px;
}


.plan_tab a {		/* 강아지 | 고양이  */
    float: left;
    width: 50%;
    height: 56px;
    line-height: 54px;
    background: #f7f9fd;
    text-align: center;
    font-size: 18px;
    font-weight: 300;
    color: #3c465e;
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    border: 1px solid #d8dce5;
    border-width: 1px 1px 1px 0;
    }
a.dog_btn.on{
background:#333333;
    color: #fff;
    font-weight: 400;
}
.main_goods_cont{
	width: 1200px;
    position: relative;
    margin: 0 auto;
    text-decoration: none;
}

dl, ul, ol, li {
	list-style: none;
}




/********* vet1 css *********/

button#btnInsert.consulting-insert{
	border: 1px solid #ffc71d;
	width: 140px;
	height: 50px;
	border-radius: 6px;
	margin-left: 1060px;
	margin-top: -20px;
	margin-bottom: 20px;
	background:#ffc71d;
	font-size: 17px;
}
div.consulting_list{
    width: 1200px;
    height: 100px;
    border-top: 1px solid #ccc;
    padding: 35px 0px 35px 0px;
    margin-bottom: 20px;
}

div.consulting-title>h1>a{
    text-decoration: none;
    color: #3b3b3b;
    font-size: 18px;
    font-family: "나눔고딕";
    }

div.consulting-item-etc-img> img#img{
	width: 40px;
    height: 40px;
}

div.consulting-item-etc{
	/* border: 2px solid blue; */
	margin-left: 50px;
	margin-top: -30px;
	
}

span.consulting-item-user {
    display: inline-block;
    width: 50px;
    height: 30px;
    color: #a3a3a3;
    font-size: 15px;
    font-weight: bold;
    }
    
span.consulting-item-date{
    display: inline-block;
    height: 30px;
    color: #757575;
    font-size: 13px;
    margin-right: 10px;
   }
    
span.consulting-item-reply{
    display: inline-block;
    height: 30px;
    color: #757575;
    font-size: 13px;
    }
    
.pick_Page{
	
	height: 100%;
	padding-bottom: 60px;
}    
    
.h1{
	text-align: center;
	padding: 35px;
	font-size: 30pt;
}   


.plan {
    width: 100%;
    overflow: hidden;
    clear: both;
    margin-bottom: 28px;
    border: 1px solid #d8dce5;
}


.plan li{		/* 강아지 | 고양이  */
    float: left;
    width: 25%;
    height: 40px;
    line-height: 40px;
    background: #f7f9fd;
    text-align: center;
    font-size: 18px;
    font-weight: 300;
    color: #3c465e;
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
   border-left: 1px solid #d8dce5;

   /* border-width: 1px 1px 1px 1px; */
    }
.plan li.dog{
    border-left: none;
} 
li.dog{
	background: white;
/*     border-color:#d8dce5; */
    color: black;
    font-weight: 400;
}
 .qna li:first-child{
 	margin-top: 55px;
 }   
.qna li{

	border-top: 1px solid #d8dce5;
	height: 80px;

	padding: 17px;
	
}   
.qna li:last-child {
	border-bottom: 1px solid #d8dce5;
}   
.qna li img {
	width: 75px;
	height: 75px;
	display: inline-block;
}    
.qna li h3{
	font-size: 16pt;
	font-weight: lighter;
	position: absolute;
	margin-top:-55px;
	margin-left:90px;

}    
.qna li h4{
	font-size:11pt;
	font-weight: lighter;
	position: absolute;
	margin-top:-20px;
	margin-left:92px;
	color: #bbbaba;

}    
.qna li h2{
	font-size: 11pt;
	font-weight: lighter;
	position: absolute;
	margin-top:-35px;
	margin-left:100px;

}        
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    