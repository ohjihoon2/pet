footer {
	border-top: 1px solid #787878;;
	background-color: #333333;
	color: white;
	clear: both;
}

footer>nav>ul {
	list-style: none;
	padding: 0px;
	text-align: center;
	margin-left: 20px;
}

footer>nav>ul li {
	display: inline;
}

footer>nav>ul li a {
	text-decoration: none;
	font-size: 13pt;
	font-weight: bold;
	padding: 0 10px 0 10px;
	border-right: 1px solid black;
	color: white;
}

footer>nav>ul li:last-child a {
	border: none;
}

footer>div span {
	color: white;
}

footer div:nth-child(3) {
	/* border:1px solid blue; */
	width: 1280px;
	margin-bottom: 100px;
	margin: 0 auto;
	text-align: center;
	/*  margin-top: 50px; */
}

footer>div:nth-child(3) img {
	width: 160px;
	height: 90px;
	position: absolute;
	margin-top: -40px;
	margin-left: -150px;
}

footer div:nth-child(3) div {
	/*  border:1px solid green */;
	width: 500px;
	margin: 0 auto;
	text-align: left;
	display: inline-block;
	font-size: 10pt;
	margin-left: 40px;
	margin-bottom: 20px;
}

