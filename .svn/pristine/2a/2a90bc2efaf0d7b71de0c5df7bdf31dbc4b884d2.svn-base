}

/**
		占쏙옙占쏙옙 占쏙옙타占쏙옙 占쏙옙占쏙옙 占싸븝옙
	**/
* {
	font-family: 'LotteMartDream';
}

body>div {
	width: :100%;
	margin: 0 auto;
}

body, header, footer {
	margin: 0px;
	padding: 0px;
}

/**
	탑占쏙옙튼
**/
button.ac-sub-go-top {
	width: 150px;
	height: 30px;
	bottom: calc(62% - 35px);
	position: fixed;
	right: calc(( 100% - 1520px)/2- 80px);
	display: inline-block;
	border: none;
	background-size: 100%;
	display: none;
	/* border:1px solid #e7e7e7; */
	/* overflow:auto; 
	  overflow-x:hidden; */
	margin: 0 auto;
	text-align: center;
}

div.ac-sub-go {
	width: 150px;
	height: 500px;
	bottom: calc(49% - 35px);
	position: fixed;
	right: 100px;
	display: inline-block;
	border: none;
	background-size: 100%;
	display: none;
	/* border:1px solid #e7e7e7; */
	/* overflow:auto; 
	  overflow-x:hidden; */
	margin: 0 auto;
	text-align: center;
}

div.ac-sub-go #topall {
	border: 1px solid #e7e7e7;
	height: 330px;
}

div.ac-sub-go #fspan1 {
	display: inline-block;
	/*border-top-right-radius:7px;*/
	/*border-top-left-radius:7px;*/
	width: 150px;
	height: 50px;
	line-height: 50px;
	color: white;
	background: #3b3b3b;
}

div.ac-sub-go .fimg1 {
	margin: 0 auto;
	width: 147px;
	height: 110px;
}

div.ac-sub-go ul {
	margin: 0px;
	padding: 0px;
	height: 100px;
}

div.ac-sub-go li {
	text-align: left;
	list-style: none;
	font-size: 10pt;
	background: #e7e7e7;
}

div.ac-sub-go .fspan3 {
	color: #2681ff;
}

div.ac-sub-go .fspan4 {
	color: red;
}

div.ac-sub-go .fspan5 {
	font-weight: bold;
}

div.ac-sub-go .fspan6 {
	font-size: 10pt;
	display: inline-block;
	/*border:1px solid red;*/
	margin-top: 5px;
}

div.ac-sub-go #fspan2 {
	padding: 1px;
	border-radius: 5px;
	background-color: #2681ff;
	color: white;
}

div.ac-sub-go a {
	text-decoration: none;
	color: black;
}

div.ac-sub-go #fdiv1 {
	/* border: 1px solid black; */
	margin: 0px;
	padding: 0px;
}

/**
			占쏙옙占�  占쏙옙타占쏙옙 占쏙옙占쏙옙 占싸븝옙	 	
		 **/
header {
	width: 100%;
	height: 220px;
	border-bottom: 2px solid black;
}

header>div:nth-child(1) {
	width: 100%;
	background: #f5f5f5;
	border-bottom: 1px solid #cccccc;
	/*border: 1px solid yellow;*/
}

header>div #d1 {
	margin: 0 auto;
	width: 1200px;
}

header>div #d1 nav {
	/*border: 1px solid black;*/
	height: 40px;
	margin: 0px;
}

header>div #d1 nav ul {
	margin: 0px;
	text-align: right;
	padding-top: 10px;
}

header>div #d1 nav ul li {
	display: inline;
}

header>div #d1 nav>ul>li:last-child {
	margin-right: 10px;
}

header>div #d1 nav>ul>li a {
	border-right: 1px solid #cccccc;
}

header>div #d1 nav>ul>li:last-child a {
	border-right: none;
}

header>div #d1 nav>ul>li>a {
	/*border-right: 1px solid #dddddd;*/
	font-size: 9pt;
	padding: 0px 10px 0px 10px;
	text-decoration: none;
	color: #222222;
}

/**
		header2
	**/
header>div:nth-child(2) {
	border-bottom: 1px solid #e5e5e5;
	padding: 15px 0px 15px 0px;
}

header>div:nth-child(2)>div {
	width: 1200px;
	margin: 0 auto;
}

header>div>div>div:nth-child(1) {
	display: inline-block;
	width: 167px;
	height: 104px;
}

header>div>div>div>a>img {
	width: 167px;
	height: 104px;
}

header>div>div>div:nth-child(2) {
	display: inline-block;
	position: absolute;
	width: 800px;
	height: 104px;
	text-align: center;
}

header>div>div>div>input[type="text"] {
	width: 600px;
	height: 50px;
	font-size: 12pt;
	padding-left: 20px;
	border: 5px solid #ffc71d;
	border-radius: 48px;
	margin-top: 20px;
	outline: none;
}

div#d2 img {
	position: absolute;
	width: 35px;
	height: 30px;
	top: 35px;
	right: 120px;
	display: inline-block;
}

/**
		header3
	**/
header>div#d3 {
	width: 1200px;
	height: 40px;
	margin: 0 auto;
	padding: 0px;
	z-index: 1;
}

header>div#d3 nav {
	position: absolute;
	top: 173px;
	height: 40px;
}

header>div#d3 nav ul {
	padding: 0px;
}

header>div#d3 nav ul li {
	display: inline-block;
	border-right: 1px solid #e5e5e5;
	border: 2px solid red;
}

header>div#d3 nav ul li:last-child {
	border-right: none;
	border: 2px solid red;
}

header>div#d3 nav ul li:last-child {
	margin-right: 10px;
	border: 2px solid red;
}

header>div#d3 nav ul li a {
	font-size: 14pt;
	text-decoration: none;
	padding: 10px 20px 7px 20px;
	color: #222222;
	border: 2px solid red;
}

#cat2 {
	display: none;
}

