.demo {
	width: 1200px;
	height: 400px;
	/* border:1px solid red; */
}

.demo .item {
	margin-bottom: 60px;
}

.content-slider {
	list-style: none outside none;
	padding-left: 0;
	margin: 0;
}

.content-slider li {
	border: 1px solid #e2e2e2;
	text-align: center;
	height: 400px;
}

.content-slider li:hover {
	border: 1px solid #ffc71d;
}

.content-slider img {
	width: 250px;
	height: 250px;
}

.content-slider li div:nth-child(1) {
	height: 250px;
}

.content-slider li div:nth-child(2) {
	height: 100px;
}

.content-slider li div:nth-child(2) .goods1 {
	display: inline-block;
	width: 80%;
	/* border:1px solid black; */
	font-size: 17px;
	font-weight: bold;
	color: #373536;
	margin-top: 20px;
}

.content-slider li div:nth-child(2) .price1 {
	display: block;
	/*border:1px solid black;*/
	font-size: 24px;
	font-weight: bold;
	color: #373536;
	margin-top: 20px;
}

/**
	 	section2 end
	 **/
section#s1>article {
	margin: 0 auto;
	width: 1920px;
	/* border: 1px solid red; */
	font-size: 35px;
	color: #373536;
	font-weight: bold;
}

section#s2>article {
	margin: 0 auto;
	width: 1920px;
	/* border: 1px solid red; */
}

section>article>div {
	display: inline-block;
	width: 280px;
	height: 300px;
	/* border: 1px solid green; */
}

section:nth-child(3)>article:nth-child(2)>div:first-child {
	display: inline-block;
	width: 580px;
	height: 350px;
	float: left;
	text-align: left;
}

div#top1>img:nth-child(1) {
	position: absolute;
	margin: 10px;
	z-index: 9999;
}

div#top1>div {
	overflow: hidden;
	border-radius: 7px;
	margin-bottom: 10px;
}

div#top1>div>a img {
	width: 580px;
	height: 350px;
}

div#top1>div>a img:hover {
	cursor: pointer;
	border-radius: 7px;
	transform: scale(1.1); /* 마우스 오버시 이미지 크기를 1.1 배만큼 확대시킨다. */
	-o-transform: scale(1.1);
	-moz-transform: scale(1.1);
	-webkit-transform: scale(1.1);
	transition: transform .35s;
	-o-transition: transform .35s;
	-moz-transition: transform .35s;
	-webkit-transition: transform .35s;
	z-index: 10;
}

div#top1>span {
	color: black;
	text-decoration: none;
	text-align: left;
	font-size: 15px;
}

section:nth-child(3)>article:nth-child(2)>div:last-child {
	display: inline-block;
	width: 580px;
	height: 350px;
	float: right;
}
