a {
	cursor: pointer;
	text-decoration: none;
	color: black;
}

div#content {
	width: 1611.8px;
	height: 1181px;
	float: right;
	border-left: 1px solid #cccccc;
	background-color: #eeeeee;
	margin: 0px;
	padding-top: 50px;
}

div#wonder_mgm {
	border: 1px solid #cccccc;
	border-radius: 10px;
	background-color: white;
	width: 1580px;
	height: 1130px;
	margin: auto;
}

div#wonder_mgm h3 {
	margin: 20px 0 0 20px;
	display: inline-block;
}

div#wonder_mgm a h4 {
	margin: 24px 10px 0 0;
	float: right;
	display: inline-block;
	font-size: 11pt;
	color: #8e8e8e;
}

div#wonder_mgm table {
	width: 1500px;
	border-spacing: 0;
	border-style: none;
	padding: 0px;
	font-family: "�������";
	font-size: 15px;
	margin: auto;
	text-align: center;
	margin-top: 25px;
}

div#wonder_mgm table tr th {
	height: 40px;
	border-top: solid 1px #888888;
	border-bottom: solid 1px #c7c7c7;
	background: #f1f1f1;
}

div#wonder_mgm table tr td {
	height: 50px;
	border-bottom: solid 1px #c7c7c7;
	
}

#wonder_mgm div.wcontents{
	width: 100%;
	height: 150px;
	overflow: auto;
}

#wonder_mgm div.wcontents img{
	width: 300px;
	height: 150px;
}
#wonder_mgm > div:nth-child(4) > table > tbody > tr > td:nth-child(4){
	width: 50px;
}
#wonder_mgm > div:nth-child(4) > table > tbody > tr > td:nth-child(5){
	width: 70px;
}
#wonder_mgm > div:nth-child(4) > table > tbody > tr > td:nth-child(6){
	width: 50px;
}
#wonder_mgm > div:nth-child(4) > table > tbody > tr > td:nth-child(7){
	width: 60px;
}
