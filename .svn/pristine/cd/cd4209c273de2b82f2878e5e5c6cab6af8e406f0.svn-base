package com.pettoday.vo;

public class WalktogetherReplyVO {
	String wal_reply_content, wal_reply_date, mem_id;
	int wal_reply_no, wal_no;

	public String getWal_reply_content() {
		return wal_reply_content;
	}

	public void setWal_reply_content(String wal_reply_content) {
		this.wal_reply_content = wal_reply_content;
	}

	public String getWal_reply_date() {
		return wal_reply_date;
	}

	public void setWal_reply_date(String wal_reply_date) {
		this.wal_reply_date = wal_reply_date;
	}

	public String getMem_id() {
		return mem_id;
	}

	public void setMem_id(String mem_id) {
		this.mem_id = mem_id;
	}

	public int getWal_reply_no() {
		return wal_reply_no;
	}

	public void setWal_reply_no(int wal_reply_no) {
		this.wal_reply_no = wal_reply_no;
	}

	public int getWal_no() {
		return wal_no;
	}

	public void setWal_no(int wal_no) {
		this.wal_no = wal_no;
	}
}
